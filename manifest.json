{
    "manifest_version": 3,
    "name": "Learning Extension",
    "version": "1.0",
    "description": "An extension that summarize your article for you and then quizzes you!",
  
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
  
    "permissions": [
      "activeTab",
      "scripting"
    ],

    "background": {
      "service_worker": "background.js"
    }
  }
  